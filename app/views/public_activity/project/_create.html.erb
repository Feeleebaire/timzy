<%= link_to project_path(a.trackable_id) do  %>
  <div class="feed-card">
 <div class="feed-avatar"> <%= activity.trackable.user.photo %></div>
    <div class="feed-context"><%= a.created_at.strftime('%-d %B %Y %H:%M:%S') %> @ <%= activity.trackable.team.name %> : </div>
     <div class="feed-details"><%= activity.trackable.title %> was added by <%= activity.trackable.user.first_name %> </div>
  </div>
<% end %>
